{
    "$schema": "node_modules/wrangler/config-schema.json",
    "name": "npkg",
    "compatibility_date": "2026-01-15",
    "compatibility_flags": ["nodejs_compat"],
    "main": "dist/_worker.js/index.js",
    "route": {
        "pattern": "npkg.lorypelli.dev",
        "custom_domain": true
    },
    "assets": {
        "binding": "ASSETS",
        "directory": "./dist"
    },
    "build": {
        "command": "pnpm build"
    }
}
